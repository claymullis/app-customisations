	<name>vim</name>
	<summary>Tim's vim</summary>
	<publish xmlns="http://gfxmonk.net/dist/0install" mode="ignore"/>
	<description>
	</description>
	<icon href="http://gfxmonk.net/dist/0install/vim-gnome/icon.svg" type="image/svg"/>
